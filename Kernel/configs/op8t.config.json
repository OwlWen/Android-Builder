[
  {
    "kernelSource": {
      "name": "OnePlus-8t_Crdroid-Kernel-15",
      "repo": "https://github.com/crdroidandroid/android_kernel_oneplus_sm8250",
      "branch": "15.0",
      "device": "kona",
      "defconfig": "vendor/kona-perf_defconfig"
    },
    "enableCcache": true,
    "toolchains": [
      {
        "repo": "https://gitlab.com/crdroidandroid/android_prebuilts_clang_host_linux-x86_clang-r547379",
        "branch": "15.0",
        "name": "crdroidandroid_clang",
        "binaryEnv": [
                    "bin"
                ]
      },
      {
        "repo": "https://gitlab.com/crdroidandroid/android_prebuilts_gcc_linux-x86_aarch64_aarch64-linux-android-4.9",
        "branch": "15.0",
        "name": "crdroidandroid_gcc",
        "binaryEnv": [
                    "bin"
                ]
      }
    ],
    "params": {
        "ARCH": "arm64",
        "CC": "clang",
        "externalCommands": {
            "CLANG_TRIPLE": "aarch64-linux-gnu-",
            "CROSS_COMPILE": "aarch64-linux-android-",
            "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
            "LD": "ld.lld",
            "AR": "llvm-ar",
            "NM": "llvm-nm",
            "OBJCOPY": "llvm-objcopy",
            "OBJDUMP": "llvm-objdump",
            "READELF": "llvm-readelf",
            "OBJSIZE": "llvm-size",
            "STRIP": "llvm-strip",
            "LDGOLD": "aarch64-linux-gnu-ld.gold",
            "LLVM_AR": "llvm-ar",
            "LLVM_DIS": "llvm-dis",
            "CONFIG_THINLTO": ""
        }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": true,
    "enableLXC": true
  }
]

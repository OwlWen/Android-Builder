[
    {
        "kernelSource": {
            "name": "OP8T_11_Horizon",
            "repo": "https://github.com/libxzr/android_kernel_oneplus_sm8250",
            "branch": "oos11/final",
            "device": "kebab",
            "defconfig": "kebab_defconfig"
        },
        "toolchains": [
            {
                "url": "https://storage.xzr.moe/horizon-clang/Horizon-Clang-16.0.6.tar.xz",
                "name": "Horizon-Clang",
                "binaryEnv": [
                  "bin"
                ]
            }
        ],
        "enableCcache": true,
        "params": {
            "ARCH": "arm64",
            "CC": "clang",
            "externalCommands": {
                "CLANG_TRIPLE": "aarch64-linux-gnu-",
                "CROSS_COMPILE": "aarch64-linux-gnu-",
                "CC": "clang",
                "CROSS_COMPILE_COMPAT": "arm-linux-gnueabi-",
                "LD": "ld.lld",
                "AR": "llvm-ar",
                "NM": "llvm-nm",
                "OBJCOPY": "llvm-objcopy",
                "OBJDUMP": "llvm-objdump",
                "STRIP": "llvm-strip",
                "LDGOLD": "aarch64-linux-gnu-ld.gold",
                "LLVM_AR": "llvm-ar",
                "LLVM_NM": "llvm-nm"
            }
        },
        "AnyKernel3": {
            "use": true,
            "release": true
        },
        "enableKernelSU": true,
        "enableLXC": true
    }
]
